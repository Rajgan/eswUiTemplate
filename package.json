{
  "name": "cob-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist coverage documentation  && yarn cache clean",
    "clean-dist": "rimraf dist",
    "clean-coverage": "rimraf coverage",
    "ng": "ng",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "ng lint",
    "lint-watch": "npm run lint --force && onchange \"src/**/*.ts\" -- npm run lint --force",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false true --gecko false --quiet",
    "compodoc": "./node_modules/.bin/compodoc -p ./tsconfig.json --hideGenerator --theme postmark --toggleMenuItems all",
    "build:docs": "npm run compodoc",
    "postinstall": "npm run webdriver-update",
    "build:dev": "ng build --dev --aot --sourcemap --show-circular-dependencies --stats-json --output-hashing all",
    "postbuild:dev": "npm run stats",
    "prebuild:transform": "ng lint",
    "build:transform": "ng build --env=transform --prod --sourcemap --stats-json --named-chunks --vendor-chunk",
    "postbuild:transform": "npm run stats",
    "prebuild:prod": "ng lint",
    "build:prod": "ng build --prod --vendor-chunk",
    "start": "ng serve --open --aot",
    "test": "ng test -cc -sr --browsers ChromeHeadless",
    "test-watch": "ng test",
    "stats": "./node_modules/.bin/webpack-bundle-analyzer ./dist/stats.json -m static -r ./coverage/bundles/bundlereport.html -O",
    "check-before-pr": "npm run clean-dist && npm run clean-coverage && npm run test && npm run build:transform"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "angular-auth-oidc-client": "^4.1.0",
    "applicationinsights-js": "^1.0.15",
    "bootstrap": "3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "ng4-loading-spinner": "^1.1.3",
    "ngx-bootstrap": "^2.0.3",
    "node-sass": "^4.9.0",
    "rxjs": "^5.5.6",
    "source-sans-pro": "^2.20.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@compodoc/compodoc": "^1.0.1",
    "@types/chai": "^3.5.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "diff": "^3.4.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tfs-reporter": "^1.0.1",
    "mocha": "^4.0.1",
    "onchange": "^3.2.1",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
